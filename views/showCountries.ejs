<style>
    table, th, td{
        border: 1px solid black;;
    }
</style>
<h1>Countries</h1>
<button><a href="\addCountries" style="text-decoration: none">Add Country</button> 
<br><br> 
<table>
    <tr>
        <th>Country Code</th>
        <th>Country Name</th>
        <th>Country Details</th>
        <th>Action</th>
    </tr>
    <% countries.forEach((country) =>
    { %>
    <tr>
        <td><%= country.co_code %></td>
        <td><%= country.co_name %></td>
        <td><%= country.co_details %></td>
        <td><a href="<%= "/edit/" + country.co_code %>">Update</a></td>
        <td><a href="<%= "/delete/" + country.co_code %>">Delete</a></td>
    </tr> 
    <% }) %>
</table>
<a href="\">Home</a>